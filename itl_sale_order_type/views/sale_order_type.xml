<odoo>
    <data>
        
        
        <record id="sot_sale_order_type_form_view_custom" model="ir.ui.view">
            <field name="name">sale.order.type.form.view.custom</field>
            <field name="model">sale.order.type</field>
            <field name="inherit_id" ref="sale_order_type.sot_sale_order_type_form_view"/>
            <field name="arch" type="xml">
                <field name="sequence_id" position="after">
                    <field name="approval_request"/>
                </field>
                <field name="journal_id" position="after">
                    <field name="sale_type_account"/>
                </field>
                <field name="incoterm_id" position="after">
                    <field name="delivery_message"/>
                </field>
            </field>
        </record>
        
    </data>
</odoo>